@model IEnumerable<PIA.Models.Propiedades>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FechaCreada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FechaActualizada)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TiposInmueble.Descripcion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ubicaciones.Calle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FechaCreada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FechaActualizada)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TiposInmueble.Descripcion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ubicaciones.Calle)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdPropiedad }) |
            @Html.ActionLink("Details", "Details", new { id=item.IdPropiedad }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdPropiedad })
        </td>
    </tr>
}

</table>
